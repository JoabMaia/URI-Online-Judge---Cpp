#include <iostream>
#include <iomanip>

using namespace std;

int main() {
  int cont=0;
  double num[6], cont_num=0;

  cout << fixed << setprecision(1);

  for(int i; i<=5; i++){
    cin >> num[i];
  }

  for(int i=0; i<=5; i++){
    if(num[i] > 0){
      cont_num += num[i];
      cont+=1;
    }
  }

  cout << cont << " valores positivos" << endl;
  cout << cont_num/cont << fixed << setprecision(1) << endl;

  return 0;
}
