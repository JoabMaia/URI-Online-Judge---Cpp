#include <iostream>

using namespace std;

int main() {
  int num, a, cont_in=0, cont_out=0;

  cin >> num;

  for(int i=0; i<num; i++){
    cin >> a;
    if(a>=10 && a<=20){
      cont_in+=1;
    }
    else{
      cont_out+=1;
    }
  }

  cout << cont_in << " in" << endl;
  cout << cont_out << " out" << endl;

  return 0;
}
