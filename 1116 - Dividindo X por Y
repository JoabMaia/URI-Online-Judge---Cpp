#include <iostream>
#include <iomanip>

using namespace std;

int main() {
  int ent;
  double num1, num2;

  cin >> ent;

  for(int i=0; i<ent; i++){
    cin >> num1 >> num2;

    if(num2 != 0){
      cout << num1/num2 << fixed << setprecision(1) << endl;
    }
    else{
      cout << "divisao impossivel" << endl;
    }
  }

  return 0;
}
