#include <iostream>
#include <stdio.h>

using namespace std;

int main(){
    double valor;
     
    cin >> valor;
    
    printf("N[%d] = %.4f\n", 0, valor);
    
    for(int i = 1; i < 100; i++){
            valor = valor/2.0;
            printf("N[%d] = %.4f\n", i, valor);
    }
    return 0;
}
