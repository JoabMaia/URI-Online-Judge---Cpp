#include <iostream>

using namespace std;

int main() {
  int num;

  cin >> num;

  while(true){

    if(num != 0){
      if(num == 0){
        break;
      }
      else{
        if(num == 1){
            cout << 1 << endl;
        }
        for(int i=1; i<num; i++){
          cout << i << " ";
          if(i == num-1){
            cout << num << endl;
            continue;
          }

        }
        cin >> num;
        continue;
      }
    }
    else{
      break;
    }
  }  

  return 0;
}
