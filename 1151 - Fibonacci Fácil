#include <iostream>

using namespace std;

  
int main(){
    int x,y, a=0,b=1,c=0;
    cin >> x;
    for(y=1; y<x; y++){
        if(y%2==1){
            cout << c << " ";
            c=a+b;
            a=c;
        }
        else if(y==2)
            cout << c << " ";
        else if(y%2==0){
            cout << c << " ";
            c=a+b;
            b=c;
        }
    }
    cout << c << endl;
    return 0;
}
