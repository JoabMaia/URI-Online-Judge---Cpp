#include <iostream>

using namespace std;

int main() {

  int j=0, contImp=0, vet[3] = {7, 6, 5};
  
  for(int i=1; i<20; i++){
      if(i%2 != 0 || contImp < 3){
        while(contImp < 3){
          cout << "I=" << i << " " << "J=" << vet[contImp] << endl;
          contImp += 1;
        }
      }
      else if(contImp == 3){
        contImp -= 3;
      }
      if(i%2 != 0 && i == 9 && contImp == 3){
        break;
      }
  }

  return 0;
}
